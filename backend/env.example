# MongoDB Configuration
MONGODB_URI=mongodb://127.0.0.1:27017/child_wellness

# Server Configuration
PORT=4000

# Razorpay Configuration (PRODUCTION)
# Get these from: https://dashboard.razorpay.com/app/keys
# For TEST mode: Use keys starting with rzp_test_
# For LIVE mode: Use keys starting with rzp_live_
RAZORPAY_KEY_ID=rzp_live_xxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=your_live_secret_key_here
RAZORPAY_WEBHOOK_SECRET=your_webhook_secret_here

# Optional: Pre-created Plan ID (if you create plan manually in Razorpay dashboard)
# If not set, plan will be created automatically
RAZORPAY_PLAN_ID=plan_xxxxxxxxxxxxx

# Free Access IDs (comma-separated) - These users won't need subscription
# Useful for testing specific users without payment
# Format: auth0|user_id_here,another_user_id
FREE_ACCESS_IDS=

# Auth0 Configuration (if using Auth0)
AUTH0_DOMAIN=your_domain.auth0.com
AUTH0_CLIENT_ID=your_client_id
AUTH0_CLIENT_SECRET=your_client_secret
