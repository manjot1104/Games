{
  "expo": {
    "name": "ChildWellness",
    "slug": "ChildWellness",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "scheme": "ChildWellness",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "EXPO_PUBLIC_AUTH0_DOMAIN": "child-wellness.us.auth0.com",
      "EXPO_PUBLIC_AUTH0_CLIENT_ID_NATIVE": "mVqiDhhSeBZ3BGUddxCvqheVb0VJcaGC",
      "EXPO_PUBLIC_AUTH0_CLIENT_ID_WEB": "pK2uzbNONOHaPUPWbD9s7Y3qx6nYntEW",
      "AUTH0_DOMAIN": "child-wellness.us.auth0.com",
      "AUTH0_CLIENT_ID_NATIVE": "mVqiDhhSeBZ3BGUddxCvqheVb0VJcaGC",
      "AUTH0_CLIENT_ID_WEB": "pK2uzbNONOHaPUPWbD9s7Y3qx6nYntEW",
      "EXPO_PUBLIC_RAZORPAY_KEY_ID": "rzp_live_RyBRdT9vru0wg0"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your camera for jaw awareness exercises. All processing happens locally on your device."
      }
    },
    "android": {
      "usesCleartextTraffic": true,
      "permissions": [
        "CAMERA"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.ChildWellness",
      "minSdkVersion": 26
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "child-wellness.us.auth0.com"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to track eye movements, measure attention, and detect jaw movements for speech therapy games. All processing happens locally on your device."
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your camera for jaw awareness exercises. All processing happens locally on your device."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
